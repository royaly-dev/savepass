{
  "name": "SavePass",
  "version": "0.0.2",
  "description": "Save pass",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "productName": "SavePass",
    "icon": "./src/assets/img/icon.ico",
    "publish": [{
      "provider": "github",
      "releaseType": "release"
    }],
    "win": {
      "target": "nsis",
      "icon": "./src/assets/img/icon.ico",
      "signtoolOptions": {
        "publisherName": "royaly"
      },
      "verifyUpdateCodeSignature": true
    }
  },
  "keywords": [],
  "author": "royaly-dev",
  "license": "gpl-3.0",
  "devDependencies": {
    "electron": "^32.1.2",
    "electron-builder": "^25.1.8"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "electron-updater": "^6.3.4"
  }
}
